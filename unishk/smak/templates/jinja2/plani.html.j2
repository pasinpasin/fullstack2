<html>

<head>

</head>



<body>


  <table class="maintab" width="100%">
    <tbody>
      <tr>
        <td align="center" colspan="4">
          <img src="https://smak.unishk.edu.al/unireports/headerImage.png" />
        </td>
      </tr>

      <tr>
        <td>
          <span class="staff">
            {{ result.plani.programi.departamenti.fakulteti.emertimi }}
          </span>
        </td>
        <td colspan="2">
          <div class="staff">
            <b>{{ result.plani.programi.emertimi }}</b>
          </div>
        </td>
        <td colspan="2">
          <div class="staff">
            Viti Akademik {{ result.plani.periudha}}
          </div>
        </td>
      </tr>

      <tr>
        <td width="30%">

        </td>
        <td colspan="2">
        </td>
        <td width="30%"></td>
      </tr>
      <tr>
        <td width="30%">
          <div class="fakultname">
            {{ result.plani.programi.departamenti.emertimi }}
          </div>
        </td>

        <td align="center" width="30%">
          <span class="staff">
            Cikli: {{ result.plani.cikli}}
          </span>
        </td>
      </tr>
    </tbody>
  </table>

  <br></br>

  <!-- shto kodin per kategorite-->
  <table class="maintab" width="100%">
   
      <tr bgcolor="#ccd4e6">
        <th width="40%">
          <span class="staff">
            <b>Tipi i veprimtarisë dhe simboli përcaktues</b>
          </span>
        </th>
        <td width="30%">
          <div class="staff">
            <b>Fusha disiplinore apo veprimtari të tjera formuese</b>
          </div>
        </td>
        <td width="10%">
          <div class="staff">
            <b>Kredite</b>
          </div>
        </td>
        <td width="10%">
          <div class="staff">
            <b>Totali krediteve</b>
          </div>
        </td>
        <td width="10%">
          <div class="staff ">
            <b>%</b>
          </div>
        </td>
      </tr>
    
    {%- set varA = namespace(value=0) -%}
    {%- set varB = namespace(value=0) -%}
    {%- set varC = namespace(value=0) -%}
    {%- set varD = namespace(value=0) -%}
    {%- set varE = namespace(value=0) -%}

    
      {%- for tipi in result.obj1 -%}

      {%- for lende in result.obj2 if lende.tipiveprimtarise == tipi.tipiveprimtarise -%}

      <tr bgcolor="#ccd4e6">
        {%- if lende.tipiveprimtarise == "A" -%}
        {%- set varA.value = varA.value + 1 -%}

        {%- if varA.value == 1 -%}

        <td rowspan="{{ tipi.total }}" width="40%">LENDE BAZE-pergatitje metodologiike dhe kulture e pergjithshme
          (Simboli
          A)</td>
        <td width="30%">{{ lende.emertimi }}</td>
        <td width="10%">{{ lende.kredite }}</td>
        <td rowspan="{{ tipi.total }}" width="10%">{{ tipi.totkrediteveprimtari }}</td>
        <td rowspan="{{ tipi.total }}" width="10%">{{ tipi.percent }}</td>
      </tr>
      {%- else -%}
      <td width="30%">{{ lende.emertimi }}</td>
      <td width="10%">{{ lende.kredite  }}</td>
      </tr>
      {%- endif -%}
      {%- elif lende.tipiveprimtarise == "B" -%}
      {%- set varB.value = varB.value + 1 -%}
      {%- if varB.value == 1 -%}
      <td rowspan="{{ tipi.total }}" width="40%">LËNDË KARAKTERIZUESE-përgatitje për disiplinën shkencore (Simboli B)
      </td>
      <td width="30%">{{ lende.emertimi }}</td>
      <td width="10%">{{ lende.kredite }}</td>
      <td rowspan="{{ tipi.total }}" width="10%">{{ tipi.totkrediteveprimtari }}</td>
      <td rowspan="{{ tipi.total }}" width="10%">{{ tipi.percent }}</td>
      </tr>
      {%- else -%}
      <td width="30%">{{ lende.emertimi }}</td>
      <td width="10%">{{ lende.kredite }}</td>
      </tr>
      {%- endif -%}

      {%- elif lende.tipiveprimtarise == "C" -%}
      {%- set varC.value = varC.value + 1 -%}
      {%- if varC.value == 1 -%}

      <td rowspan="{{ tipi.total }}" width="40%">LËNDË NDËRDISIPLINORE/INTEGRUESE-nëndidiplina, profile dhe grup-lëndë
        me zgjedhje (Simboli C)</td>
      <td width="30%">{{ lende.emertimi }}</td>
      <td width="10%">{{ lende.kredite }}</td>
      <td rowspan="{{ tipi.total }}" width="10%">{{ tipi.totkrediteveprimtari }}</td>
      <td rowspan="{{ tipi.total }}" width="10%">{{ tipi.percent }}</td>
      </tr>
      {%- else -%}
      <td width="30%">{{ lende.emertimi }}</td>
      <td width="10%">{{ lende.kredite }}</td>
      </tr>
      {%- endif -%}
      {%- elif lende.tipiveprimtarise == "D" -%}
      {%- set varD.value = varD.value + 1 -%}
      {%- if varD.value == 1 -%}
      <td rowspan="{{ tipi.total }}" width="40%">LËNDË PLOTËSUESE-gjuhë të huaja, njohuri informatike, praktika
        profesionale (Simboli D)</td>
      <td width="30%">{{ lende.emertimi }}</td>
      <td width="10%">{{ lende.kredite }}</td>
      <td rowspan="{{tipi.total}}" width="10%">{{ tipi.totkrediteveprimtari }}</td>
      <td rowspan="{{tipi.total}}" width="10%">{{ tipi.percent }}</td>
      </tr>
      {%- else -%}
      <td width="30%">{{ lende.emertimi }}</td>
      <td width="10%">{{ lende.kredite }}</td>
      </tr>
      {%- endif -%}
      {%- else -%}
      {%- set varE.value = varE.value + 1 -%}
      {%- if varE.value == 1 -%}
      <td rowspan="{{ tipi.total }}" width="40%">DETYRIME PËRMBYLLËSE. (Simboli E)</td>
      <td width="30%">{{ lende.emertimi }}</td>
      <td width="10%">{{ lende.kredite }}</td>
      <td rowspan="{{ tipi.total }}" width="10%">{{ tipi.totkrediteveprimtari }}</td>
      <td rowspan="{{ tipi.total }}" width="10%">{{ tipi.percent }} </td>
      </tr>
      {%- else -%}
      <td width="30%">{{ lende.emertimi }}</td>
      <td width="10%">{{ lende.kredite }}</td>
      </tr>
      {%- endif -%}



      {%- endif -%}
      {%- endfor -%}

      {%- endfor -%}



      <tr bgcolor="#eef1f7" style="page-break-inside:avoid;">
        <th colspan="3" align="left">
          Totali
        </th>
        {{result.totkredite.totKredite}}
        <th width="10%">

        </th>
        <th width="10%">
          {{result.finaltotal_percent}}
        </th>
      </tr>
   
  </table>

  <br></br>

  <table class="maintab" width="100%">


    {%- for ngarkesa in result.obj3 -%}
    <tr>
      <td colspan="3">

        <table class="maintab" width="100%">
          <tr>
            <td colspan="5" align="center">
              <b>Viti {{ ngarkesa.viti }}</b>
            </td>
          </tr>
          <tr>
            <td align="left" colspan="5">

              {%- if ngarkesa.totkreditepervit>60 -%}
              <span>
                <b>
                  Numri Total i krediteve:
                  <span style="color:#bf4040;">
                    <b>{{ ngarkesa.totkreditepervit }}</b>
                  </span>
                </b>
              </span>
              {%- else -%}

              <span>
                <b>
                  Numri Total i krediteve
                  <span style="color:#0080ff;">
                    <b>{{ ngarkesa.totkreditepervit }}</b>
                  </span>
                </b>
              </span>
              {%- endif -%}
            </td>

          </tr>
          <tr>
            <td width="25%"></td>
            <td align="center" width="24%">
              <b>Semestri I</b>
            </td>
            <td width="2%"></td>
            <td align="center" width="24%">
              <b>Semestri II</b>
            </td>
            <td width="25%"></td>
          </tr>

          <tr>
            <td width="25%"></td>
            <td align="center" width="24%">
              {%- if ngarkesa.ngarkesasem1>25 -%}
              <span>
                Ngarkesa mesimore: <span style="color:#bf4040;">
                  <b>{{ ngarkesa.ngarkesasem1 }}</b>
                </span>
              </span>


              {%- else -%}
              Ngarkesa mesimore: <span style="color:#0080ff;">
                <b>{{ ngarkesa.ngarkesasem1 }}/b>
              </span>
              </span>

              {%- endif -%}

            </td>
            <td width="2%"></td>
            <td align="center" width="24%">
              {%- if ngarkesa.ngarkesasem2>25 -%}
              <span>
                Ngarkesa mesimore: <span style="color:#bf4040;">
                  <b>{{ ngarkesa.ngarkesasem2 }}</b>
                </span>
              </span>


              {%- else -%}
              Ngarkesa mesimore: <span style="color:#0080ff;">
                <b>{{ ngarkesa.ngarkesasem2 }}</b>
              </span>
              </span>

              {%- endif -%}

              <br></br>
            </td>
            <td width="25%"></td>
          </tr>

        </table>
        <br></br>
        <table class="maintab" width="100%">

          <tr>
            <th bgcolor="#ccd4e6" rowspan="2">Nr</th>
            <th bgcolor="#ccd4e6" rowspan="2">Emertimi Lendes</th>
            <th bgcolor="#ccd4e6" rowspan="2">Tipi Veprimtarise</th>
            <th bgcolor="#ccd4e6" rowspan="2">Kredite</th>
            <th bgcolor="#ccd4e6" colspan="5">Semestri I</th>
            <th bgcolor="#ccd4e6" colspan="5">Semestri II</th>
            <th bgcolor="#ccd4e6" colspan="4">Totali</th>
            <th bgcolor="#ccd4e6" width="4%" rowspan="2">Ore jashte<br></br>auditorit</th>
            <th bgcolor="#ccd4e6" colspan="2">Semestri</th>
          </tr>
          <tr>

            <th bgcolor="#ccd4e6" width="4%">Nr Jave</th>
            <th bgcolor="#ccd4e6">Leks</th>
            <th bgcolor="#ccd4e6">Sem</th>
            <th bgcolor="#ccd4e6">Lab</th>
            <th bgcolor="#ccd4e6" width="3%">Prakt</th>
            <th bgcolor="#ccd4e6" width="4%">Nr Jave</th>
            <th bgcolor="#ccd4e6">Leks</th>
            <th bgcolor="#ccd4e6">Sem</th>
            <th bgcolor="#ccd4e6">Lab</th>
            <th bgcolor="#ccd4e6" width="3%">Prakt</th>
            <th bgcolor="#ccd4e6">Leks</th>
            <th bgcolor="#ccd4e6">Sem</th>
            <th bgcolor="#ccd4e6">Lab</th>
            <th bgcolor="#ccd4e6">Prakt</th>
            <th bgcolor="#ccd4e6"> I </th>
            <th bgcolor="#ccd4e6">II</th>
          </tr>
          {%- for lendet in result.obj2 if lendet.viti==ngarkesa.viti -%}
          <tr>
            <td bgcolor="#eef1f7">
              {{ lendet.renditja}}
            </td>
            <td bgcolor="#eef1f7">
              {{ lendet.emertimi }}
            </td>
            <td bgcolor="#eef1f7">
              {{ lendet.tipiveprimtarise }}
            </td>
            <td bgcolor="#eef1f7" align="center">
              {{ ("%.2f"|format(lendet.kredite)).rstrip('0').rstrip('.')  }}
            </td>
            <td bgcolor="#eef1f7" align="center">
              {{ lendet.nrjavesem1 }}
            </td>
            <td bgcolor="#eef1f7" align="center">
              {{ lendet.leksionesem1 }}
            </td>
            <td bgcolor="#eef1f7" align="center">
              {{ lendet.seminaresem1 }}
            </td>
            <td bgcolor="#eef1f7" align="center">
              {{ lendet.laboratoresem1 }}
            </td>
            <td bgcolor="#eef1f7" align="center">
              {{ lendet.praktikasem1 }}
            </td>
            <td bgcolor="#eef1f7" align="center">
              {{ lendet.nrjavesem2 }}
            </td>
            <td bgcolor="#eef1f7" align="center">
              {{ lendet.leksionesem2 }}
            </td>
            <td bgcolor="#eef1f7" align="center">
              {{ lendet.seminaresem2 }}
            </td>
            <td bgcolor="#eef1f7" align="center">
              {{ lendet.laboratoresem2 }}
            </td>
            <td bgcolor="#eef1f7" align="center">
              {{ lendet.praktikasem2 }}
            </td>
            <td bgcolor="#eef1f7" align="center">
              {{ lendet.totleksione }}
            </td>
            <td bgcolor="#eef1f7" align="center">
              {{ lendet.totseminare }}
            </td>
            <td bgcolor="#eef1f7" align="center">
              {{ lendet.totlaboratore }}
            </td>
            <td bgcolor="#eef1f7" align="center">
              {{ lendet.totpraktika }}
            </td>
            <td bgcolor="#eef1f7" align="center">

            </td>
            <td bgcolor="#eef1f7" align="center">
              {{ lendet.semestri1 }}
            </td>
            <td bgcolor="#eef1f7" align="center">
              {{ lendet.semestri2 }}
            </td>
          </tr>

          {%- endfor -%}

          {%- if result.zgjedhje is defined and result.zgjedhje|length > 0 -%}
          <tr>
            <td colspan="21" align="Center">
              <br></br>
              <table width="50%" style="break-after: page;">
                <tr>

                  <th bgcolor="#ccd4e6">Emërtimi i Lëndës</th>
                  <th bgcolor="#ccd4e6">Tipi i Veprimtarisë</th>
                </tr>
                {%- for lendemezgjedhje in result.zgjedhje if lendemezgjedhje.lenda.viti==ngarkesa.viti -%}
                <tr >
                  <td bgcolor="#eef1f7">
                    {{ lendemezgjedhje.emertimi }}
                  </td>
                  <td bgcolor="#eef1f7">
                    {{ lendemezgjedhje.lenda.emertimi }}
                  </td>

                </tr>
                {%- endfor -%}
              </table>
              <br></br>
            </td>


          </tr>
          {%- endif -%}


          <tr>
            <td colspan="21">
              <br></br>
            </td>
          </tr>

        </table>
        <br></br>
        <br></br>


      </td>
    </tr>
    {%- endfor -%}
    <tr>
      <td colspan="3" align="right">
        <br></br>
        <br></br>
        <div class="footerdiv">
          <div class="staffname2">
            Drejtori i departamentit
          </div>
          <br></br>
          <br></br>
          <div class="staffname2">

          </div>
          <br></br>
          <br></br>

        </div>
      </td>
    </tr>
  </table>






</body>

</html>